<table class="table table-striped">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var user in Model)
    {
      <tr>
        <td>@user.Name</td>
        <td>@user.Email</td>
        <td>@user.Status</td>
      </tr>
    }
  </tbody>
</table>
